<%- include("../partials/header") %>
<div class="container">
<div class="row">
    <h1 style="text-align: center; padding-bottom: 20px;">Create a new campground</h1>
<div style="width: 30%; margin: 0 auto;">
<form action="/campgrounds" method="POST">
    <div class="form-group">
        <input class="form-control" type="text" name="searchterm" placeholder="Campground Name">
    </div>
    <div class="form-group">
        <input class="form-control" type="text" name="imglink" placeholder="Image Link">
    </div>
    <div class="form-group">
        <input class="form-control" type="text" name="campdescription" placeholder="Camp Description">
    </div>
    <div class="form-group">
        <button class="btn btn-primary btn-block btn-lg btn-default">Submit</button>
    </form>
</form>

</div>
<a href="/">Go Back</a>
</div>
</div>
<%- include("../partials/footer") %>
